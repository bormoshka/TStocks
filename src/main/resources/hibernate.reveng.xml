<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering PUBLIC "-//Hibernate/Hibernate Reverse Engineering DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.0.dtd">
<hibernate-reverse-engineering>
  <schema-selection match-catalog="inventory"/>
  <table-filter match-name="UnitType_has_Property"/>
  <table-filter match-name="Transaction"/>
  <table-filter match-name="Property"/>
  <table-filter match-name="Location"/>
  <table-filter match-name="Category"/>
  <table-filter match-name="LocationType"/>
  <table-filter match-name="UnitPropertyValue"/>
  <table-filter match-name="UnitType"/>
  <table-filter match-name="Unit"/>
</hibernate-reverse-engineering>
